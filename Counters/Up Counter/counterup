:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/va_math.vpi";
S_0x11cf06030 .scope module, "counter_up_tb" "counter_up_tb" 2 1;
 .timescale 0 0;
v0x11cf1ab50_0 .var "clock", 0 0;
v0x11cf1abf0_0 .net "count", 3 0, v0x11cf1a9a0_0;  1 drivers
v0x11cf1aca0_0 .var "reset", 0 0;
S_0x11cf04b60 .scope module, "DUT" "counter_up" 2 10, 3 1 0, S_0x11cf06030;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clock";
    .port_info 1 /INPUT 1 "reset";
    .port_info 2 /OUTPUT 4 "count";
v0x11cf04cd0_0 .net "clock", 0 0, v0x11cf1ab50_0;  1 drivers
v0x11cf1a9a0_0 .var "count", 3 0;
v0x11cf1aa50_0 .net "reset", 0 0, v0x11cf1aca0_0;  1 drivers
E_0x11cf06530 .event posedge, v0x11cf1aa50_0, v0x11cf04cd0_0;
    .scope S_0x11cf04b60;
T_0 ;
    %wait E_0x11cf06530;
    %load/vec4 v0x11cf1aa50_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %store/vec4 v0x11cf1a9a0_0, 0, 4;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x11cf1a9a0_0;
    %addi 1, 0, 4;
    %store/vec4 v0x11cf1a9a0_0, 0, 4;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x11cf06030;
T_1 ;
    %delay 5, 0;
    %load/vec4 v0x11cf1ab50_0;
    %inv;
    %store/vec4 v0x11cf1ab50_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x11cf06030;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x11cf1ab50_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x11cf1aca0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x11cf1aca0_0, 0, 1;
    %delay 300, 0;
    %vpi_call 2 22 "$finish" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x11cf06030;
T_3 ;
    %vpi_call 2 27 "$dumpfile", "upcounter4bit.vcd" {0 0 0};
    %vpi_call 2 28 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x11cf06030 {0 0 0};
    %vpi_call 2 29 "$monitor", $time, " Reset = %b | Count = %b", v0x11cf1aca0_0, v0x11cf1abf0_0 {0 0 0};
    %end;
    .thread T_3;
