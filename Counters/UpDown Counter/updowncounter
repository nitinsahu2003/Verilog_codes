    .port_info 0 /INPUT 1 "clock";
    .port_info 1 /INPUT 1 "reset";
    .port_info 2 /INPUT 1 "mode";
    .port_info 3 /OUTPUT 4 "count";
v0x123f044b0_0 .net "clock", 0 0, v0x123f147c0_0;  1 drivers
v0x123f14560_0 .var "count", 3 0;
v0x123f14610_0 .net "mode", 0 0, v0x123f14910_0;  1 drivers
v0x123f146c0_0 .net "reset", 0 0, v0x123f149e0_0;  1 drivers
E_0x123f04450 .event posedge, v0x123f146c0_0, v0x123f044b0_0;
    .scope S_0x123f04220;
T_0 ;
    %wait E_0x123f04450;
    %load/vec4 v0x123f146c0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x123f14560_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x123f14610_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %load/vec4 v0x123f14560_0;
    %addi 1, 0, 4;
    %assign/vec4 v0x123f14560_0, 0;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v0x123f14560_0;
    %subi 1, 0, 4;
    %assign/vec4 v0x123f14560_0, 0;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x123f040a0;
T_1 ;
    %delay 5, 0;
    %load/vec4 v0x123f147c0_0;
    %inv;
    %store/vec4 v0x123f147c0_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x123f040a0;
T_2 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x123f149e0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x123f147c0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x123f14910_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x123f149e0_0, 0, 1;
    %delay 100, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x123f14910_0, 0, 1;
    %delay 100, 0;
    %vpi_call 2 31 "$finish" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x123f040a0;
T_3 ;
