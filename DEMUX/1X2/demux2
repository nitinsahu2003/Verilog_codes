S_0x139f06ce0 .scope module, "demux1to2_tb" "demux1to2_tb" 2 1;
 .timescale 0 0;
v0x139f1bbf0_0 .var "in", 0 0;
v0x139f1bc90_0 .var "sel", 0 0;
v0x139f1bd40_0 .net "y0", 0 0, v0x139f1ba40_0;  1 drivers
v0x139f1be10_0 .net "y1", 0 0, v0x139f1baf0_0;  1 drivers
S_0x139f06e50 .scope module, "DUT" "demux1to2" 2 10, 3 1 0, S_0x139f06ce0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "in";
    .port_info 1 /INPUT 1 "sel";
    .port_info 2 /OUTPUT 1 "y0";
    .port_info 3 /OUTPUT 1 "y1";
v0x139f056f0_0 .net "in", 0 0, v0x139f1bbf0_0;  1 drivers
v0x139f1b9a0_0 .net "sel", 0 0, v0x139f1bc90_0;  1 drivers
v0x139f1ba40_0 .var "y0", 0 0;
v0x139f1baf0_0 .var "y1", 0 0;
E_0x139f07540 .event anyedge, v0x139f1b9a0_0;
    .scope S_0x139f06e50;
T_0 ;
    %wait E_0x139f07540;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x139f1ba40_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x139f1baf0_0, 0, 1;
    %load/vec4 v0x139f1b9a0_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 1;
    %cmp/u;
    %jmp/1 T_0.0, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 1;
    %cmp/u;
    %jmp/1 T_0.1, 6;
    %jmp T_0.2;
T_0.0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x139f1ba40_0, 0, 1;
    %jmp T_0.2;
T_0.1 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x139f1baf0_0, 0, 1;
    %jmp T_0.2;
T_0.2 ;
    %pop/vec4 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x139f06ce0;
T_1 ;
    %vpi_call 2 15 "$dumpfile", "demux2bit.vcd" {0 0 0};
    %vpi_call 2 16 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x139f06ce0 {0 0 0};
    %vpi_call 2 17 "$monitor", $time, " in = %b | sel = %b | y0 = %b | y1 = %b", v0x139f1bbf0_0, v0x139f1bc90_0, v0x139f1bd40_0, v0x139f1be10_0 {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x139f06ce0;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x139f1bbf0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x139f1bc90_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x139f1bc90_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x139f1bbf0_0, 0, 1;
