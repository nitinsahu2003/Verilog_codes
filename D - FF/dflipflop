S_0x151f05740 .scope module, "DUT" "d_ff" 2 10, 3 1 0, S_0x151f068f0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clock";
    .port_info 1 /INPUT 1 "D";
    .port_info 2 /OUTPUT 1 "Q";
v0x151f06a60_0 .net "D", 0 0, v0x151f1b130_0;  1 drivers
v0x151f1af90_0 .var "Q", 0 0;
v0x151f1b030_0 .net "clock", 0 0, v0x151f1b280_0;  1 drivers
E_0x151f06cb0 .event posedge, v0x151f1b030_0;
    .scope S_0x151f05740;
T_0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x151f1af90_0, 0, 1;
    %end;
    .thread T_0;
    .scope S_0x151f05740;
T_1 ;
    %wait E_0x151f06cb0;
    %load/vec4 v0x151f06a60_0;
    %assign/vec4 v0x151f1af90_0, 0;
    %jmp T_1;
    .thread T_1;
    .scope S_0x151f068f0;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x151f1b280_0, 0, 1;
T_2.0 ;
    %delay 5, 0;
    %load/vec4 v0x151f1b280_0;
    %inv;
    %store/vec4 v0x151f1b280_0, 0, 1;
    %jmp T_2.0;
    %end;
    .thread T_2;
    .scope S_0x151f068f0;
T_3 ;
    %vpi_call 2 21 "$dumpfile", "dffwaveform.vcd" {0 0 0};
    %vpi_call 2 22 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x151f068f0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x151f1b130_0, 0, 1;
    %delay 3, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x151f1b130_0, 0, 1;
    %delay 4, 0;
    %delay 2, 0;
    %vpi_call 2 30 "$display", "D = %b | Q = %b", v0x151f1b130_0, v0x151f1b1d0_0 {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x151f1b130_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 34 "$display", "D = %b | Q = %b", v0x151f1b130_0, v0x151f1b1d0_0 {0 0 0};
    %vpi_call 2 36 "$finish" {0 0 0};
